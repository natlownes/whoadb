{
  "name": "whoadb",
  "author": "Nat Lownes <nat.lownes@gmail.com>",
  "description": "Store and read JSON objects in a flat file if you want!",
  "version": "0.1.3",
  "main": "./lib/index.js",
  "dependencies": {
    "fs-extra": "~0.4.0",
    "node-uuid": "~1.4.0"
  },
  "devDependencies": {
    "coffee-script": "~1.6.0",
    "honk-cake": ">=0.0.5",
    "mocha": "~1.8.1",
    "chai": "~1.4.2"
  },
  "scripts": {
    "test": "cake test",
    "test:watch": "cake test:watch",
    "build": "find ./src -iname '*.coffee' | xargs coffee -c -o lib"
